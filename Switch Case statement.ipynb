    "from typing import *\n",
    "import math\n",
    "def areaSwitchCase(ch: int, a: List[float]):\n",
    "    area=0.0\n",
    "    if ch==1:\n",
    "        area = math.pi*a[0]*a[0]\n",
    "    if ch==2:\n",
    "        area = a[0]*a[1]\n",
    "    return \"{:.5f}\".format(area)\n"
