   "id": "148676a5",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "15\n",
      "Teenager\n"
     ]
    }
   ],
   "source": [
    "n = int(input())\n",
    "if n >=0 and n<=2:\n",
    "    print(\"Baby\")\n",
    "elif n>=3 and n<=14:\n",
    "    print(\"child\")\n",
    "elif n>=15 and n<=22:\n",
    "    print(\"Teenager\")\n",
    "else:\n",
    "    print(\"Adult\")"
