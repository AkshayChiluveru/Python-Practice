   "source": [
    "n = int(input())\n",
    "even = 0\n",
    "odd = 0\n",
    "while n>0:\n",
    "    digit = n%10\n",
    "    if n%2==0:\n",
    "        even += digit\n",
    "    else:\n",
    "        odd += digit\n",
    "    n = n//10\n",
    "\n",
    "print(\"Even is:\",even,\"Odd is:\",odd)"
   ]
